<?xml version='1.0' encoding='utf-8'?>
<widget id="org.collaborate.directory" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Collaborate</name>
    <description>
        Employee directory to maintain all the details of employee.
    </description>
    <author email="mohanramphp@gmail.com">
        Mohan Ram
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
